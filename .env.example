# Mouv'Minute - Variables d'environnement

# Timer
VITE_ENABLE_TIMER=true

# Tooltips (pour debug crash TooltipProvider)
VITE_ENABLE_TOOLTIP=true

# Notifications Push - Clé publique VAPID
# Générez une paire de clés avec: npx web-push generate-vapid-keys
# Copiez ici la clé publique (celle qui commence souvent par "B")
VITE_VAPID_PUBLIC_KEY=your_vapid_public_key_here

# Mode développement
NODE_ENV=development